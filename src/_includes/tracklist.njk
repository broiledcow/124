      
      <h5 style="inline">Tracks on "{{ name }}"
        <br><sup><sup><sup><mark>{{tracklistnotes}}</mark></sup></sup></sup>
        <sup><br><mark>{{side1}}</mark> {% for tag in tracklistA -%}
        {% if tag !== 'posts' %}
          <br><span class="tag">{{ tag }}</span>
        {% endif %}
      {%- endfor %}
        </sup>
        <sup><br><mark>{{side2}}</mark>
        <sup>{% for tag in tracklistB -%}
        {% if tag !== 'posts' %}
          <br><span class="tag">{{ tag }}</span>
        {% endif %}
      {%- endfor %}
        </sup>
    </h5>

                    {% for tag in tags %}
                {%- set tagColl = collections[tag] | excludeFromCollection(page.url) -%}
                    {%- if tagColl | length %}
                        <h2>Music by {{ tag }}</h2>
                        <ul>
                            {%- for post in tagColl -%}
                                <li><a href="{{ post.url }}">{{ post.data.name }}</a></li>
                            {%- endfor -%}
                        </ul>
                    {%- endif %}
                {% endfor %}
