              <h5>Releases by {{name}}</h5>
                {%- for post in collections[tag] -%}
                    {% if post.data.artist !== this.page.data.name %}
                        <li>
                            <a href="{{ post.url }}">
                                {{ post.data.name }} - {{ post.data.year }}
                            </a>
                        </li>
                    {% endif %}
                {%- endfor -%}